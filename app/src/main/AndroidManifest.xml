<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="co.WishRoll">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".homepage"></activity>
        <activity android:name=".searchpage" />
        <activity android:name=".setpassword" />
        <activity android:name=".setfull" />
        <activity android:name=".setage" />
        <activity android:name=".setusername" />
        <activity android:name=".register" />
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".notificationpage"></activity>
        <activity android:name=".profilepage"></activity>
        <activity android:name=".uploadpage"></activity>

    </application>

</manifest>