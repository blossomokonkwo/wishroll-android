<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="co.WishRoll">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".Home.HomeActivity"></activity>
        <activity android:name=".Search.SearchActivity" />
        <activity android:name=".Signup.PasswordActivity" />
        <activity android:name=".Signup.NameActivity" />
        <activity android:name=".Signup.AgeActivity" />
        <activity android:name=".Signup.UsernameActivity" />
        <activity android:name=".Signup.RegisterActivity" />
        <activity android:name=".Signup.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".Notifications.NotificationActivity"></activity>
        <activity android:name=".Profile.ProfileActivity"></activity>
        <activity android:name=".Upload.UploadActivity"></activity>

    </application>

</manifest>